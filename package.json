{
  "name": "koilang-project",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "clean": "rm -rf build server/dist",
    "dev": "vite",
    "dev-debug": "vite --debug",
    "build": "tsc -b && vite build",
    "server": "source server/.venv/bin/activate && python3 server/server.py",
    "server-background": "sudo nohup npm run server & && disown",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm run test-with-coverage",
    "test": "vitest run",
    "test-with-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "axios": "^1.7.4",
    "formik": "^2.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "3.3.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
